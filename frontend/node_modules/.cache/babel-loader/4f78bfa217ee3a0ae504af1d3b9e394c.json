UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA;AAIA;AAdA;AAbA,CAAA","sourcesContent":["<template>\n    <b-navbar toggleable=\"lg\" type=\"dark\" variant=\"info\">\n      <b-navbar-brand href=\"#\">Otherside</b-navbar-brand>\n\n      <b-navbar-toggle target=\"nav-collapse\"></b-navbar-toggle>\n\n      <b-collapse id=\"nav-collapse\" is-nav>\n\n        <!-- Right aligned nav items -->\n\n        <b-navbar-nav class=\"ml-auto\">\n          <b-nav-item-dropdown right>\n            <!-- Using 'button-content' slot -->\n            <template v-slot:button-content>\n              <span v-if=\"currentUser\"> \n                {{currentUser}}\n              </span>\n              <span v-else>\n                My Account\n              </span>\n            </template>\n            <b-dropdown-item href=\"#\">Profile</b-dropdown-item>\n            <b-dropdown-item href=\"#\">Sign Out</b-dropdown-item>\n          </b-nav-item-dropdown>\n        </b-navbar-nav>\n      </b-collapse>\n    </b-navbar>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'AppHeader',\n  props: ['token', 'apiAddress'],\n  data () {\n    return {\n      currentUser: null\n    }\n  },\n  mounted () {\n    if (this.token && this.apiAddress != null && this.currentUser == null) {\n      this.getCurrentUser();\n    }\n  },\n  methods: {\n    getCurrentUser: function() {\n      const auth = {\n        headers: {\n          'Authorization': `Bearer ${this.token}`\n        }\n      }\n\n      var currentUserURL = `${this.apiAddress}/user`;\n      \n      axios.get(currentUserURL, auth)\n        .then((response => {\n          this.currentUser = response.data;\n        }));\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components"}]}